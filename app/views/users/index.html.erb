    <% @users.each do |user| %>
        <h3><%= user.name %></h3>
        <p><%= user.email %></p>
        <p><%= link_to 'Show', user_path(user) %></p> | <%= link_to 'Edit', edit_user_path(user) %>
        <p><%= link_to 'Destroy', user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %></p>
    <% end %>
<%= link_to 'New User', new_user_path %>
